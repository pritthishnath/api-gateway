{
  "services": {
    "auth": {
      "url": "https://auth.pnath.in",
      "endpoints": {
        "login": { "url": "/login" },
        "register": { "url": "/register" },
        "refresh": { "url": "/refresh" },
        "logout": { "url": "/logout" },
        "user": { "url": "/user" },
        "server-key": { "url": "/server-key" }
      }
    },
    "resources": {
      "keeper": {
        "url": "https://api.keeper.pnath.in",
        "endpoints": {
          "note": { "url": "/note", "scopes": ["share"] },
          "tag": { "url": "/tag" },
          "shared": { "url": "/shared" }
        }
      }
    }
  }
}
